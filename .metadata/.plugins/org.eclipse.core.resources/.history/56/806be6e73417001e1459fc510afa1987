package com.baekjoon.run;

import java.util.Scanner;

public class WatchRun {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int h = sc.nextInt();
		int m = sc.nextInt();
		
		int num = m - 45;
		int num2 = 0;
		
		if(num < 0) {
			num2 = h - 1;
			
		}
		
		sc.close();

	}

}
